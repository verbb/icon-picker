{
    "name": "verbb/icon-picker",
    "description": "A slick field to pick icons from. Supports SVGs, Sprites, Webfonts, Font Awesome and more.",
    "type": "craft-plugin",
    "version": "3.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "icon-picker"
    ],
    "support": {
        "email": "support@verbb.io",
        "issues": "https://github.com/verbb/icon-picker/issues?state=open",
        "source": "https://github.com/verbb/icon-picker",
        "docs": "https://github.com/verbb/icon-picker",
        "rss": "https://github.com/verbb/icon-picker/commits/v2.atom"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Verbb",
            "homepage": "https://verbb.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "cakephp/core": "^5.0.0",
        "cakephp/utility": "^5.0.0",
        "craftcms/cms": "^5.0.0",
        "glenscott/url-normalizer": "^1.4",
        "nystudio107/craft-plugin-vite": "^5.0.0",
        "phenx/php-font-lib": "^0.5.0",
        "verbb/base": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "verbb\\iconpicker\\": "src/"
        }
    },
    "extra": {
        "name": "Icon Picker",
        "handle": "icon-picker",
        "changelogUrl": "https://raw.githubusercontent.com/verbb/icon-picker/craft-5/CHANGELOG.md",
        "class": "verbb\\iconpicker\\IconPicker"
    }
}
